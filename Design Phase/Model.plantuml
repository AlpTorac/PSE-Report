    + Event getAction(char)
    }
    enum Operation{
        LESS
        EQUAL
        BIGGER
    }
    enum Mode{
        MAX
        MIN
        AVG
        MED
    }
    class Filter{
        - {static} ArrayList<Filter> filters
        - Attribute attribute
        - Mode mode
        - float value
        - Operation operation
        __
        + Filter(Attribute, Mode, float,
        Operation)
        + {static} void filter()
        + {static} void saveDefaultFilters()
        + {static} void loadDefaultFilters()
    }
}
