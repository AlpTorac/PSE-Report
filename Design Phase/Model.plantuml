    class Language{
        - String[] liberty
        - String[] menus
        - String[] methods
        - String[] attributes
        - String[] errors
        - String[] misc
        __
        + getLiberty(): String
        + getElements(): String[]
        + getMenus(): String[]
        + getMethods(): String[]
        + getAttributes(): String[]
        + getErrors(): String[]
