        + void saveFile(String, String, String)
    }
    class Interpolator{
        __
        + Interpolator()
        + {static} float[] interpolate(float[], float[], float[])
        + {static} float[][] bicubicInterpolate(float[], float[], float[][], float[], float[])
    }
